{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "shreeln",
  "version": "0.1.0",
  "identifier": "com.babith.app",
  "build": {
    "beforeDevCommand": "bun cross-env BROWSER=none bun run dev",
    "devUrl": "http://localhost:1420",
    "beforeBuildCommand": "bun run build",
    "frontendDist": "../dist"
  },
  "app": {
    "windows": [
      {
        "title": "shreeln",
        "minWidth": 800,
        "minHeight": 600,
        "width": 1400,
        "height": 700
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "publisher": "Trikona Tech",
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  }
}
