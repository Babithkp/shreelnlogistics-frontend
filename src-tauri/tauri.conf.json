{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "shreeln",
  "version": "0.1.3",
  "identifier": "com.babith.app",
  "build": {
    "beforeDevCommand": "bun cross-env BROWSER=none bun run dev",
    "devUrl": "http://localhost:1420",
    "beforeBuildCommand": "bun run build",
    "frontendDist": "../dist"
  },
  "app": {
    "windows": [
      {
        "title": "shreeln",
        "minWidth": 800,
        "minHeight": 600,
        "width": 1400,
        "height": 700
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "publisher": "Trikona Tech",
    "active": true,
    "createUpdaterArtifacts": true,
    "targets": [
      "nsis",
      "msi"
    ],
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  },
  "plugins": {
    "updater": {
      "active": true,
      "endpoints": [
        "https://shreeln-backend.vercel.app/tauri-update/{{target}}/{{currentVersion}}"
      ],
      "dialog": true,
      "pubkey": "dW50cnVzdGVkIGNvbW1lbnQ6IG1pbmlzaWduIHB1YmxpYyBrZXk6IEVBNUMzRUMwNTJEQ0I1OEQKUldTTnRkeFN3RDVjNm5wd3NqU3o2aHJ2N090TkNkSFpmaEYzQ0pZaFR0bVRyL2Y5Y0dRWi9FbkIK"
    }
  }
}